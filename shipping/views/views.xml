<odoo>
  <data>
    <!-- explicit list view definition -->


    <!-- sale.order inherit form view -->
    <record id="view_id_sale_order_inherit" model="ir.ui.view">
      <field name="name">sale.order.inherit.view.form</field>
      <field name="model">sale.order</field>
      <field name="inherit_id" ref="sale.view_order_form"/>
      <field name="arch" type="xml">
        <!-- Add your xpath here -->
            <xpath expr="//field[@name='state']" position="replace">
                <field name="state" widget="statusbar"/>
            </xpath>
            <xpath expr="//button[@name='action_confirm']" position="after">
               <button name="unloading" states="sale" type="object"
                        string="Unloading"/>
            </xpath>

             <xpath expr="//button[@name='action_cancel']" position="after">
               <button name="done" states="unloading" type="object"
                         string="Done"/>
            </xpath>
            <xpath expr="//div[@name='button_box']" position="inside">
              <!-- Add your fields here -->
                <button class="oe_stat_button" name="document_view" type="object" icon="fa-list-ol">
                    <field string="Documents" name="document_count" widget="statinfo"/>
                </button>
            </xpath>
            <xpath expr="//div[@name='button_box']" position="inside">
              <!-- Add your fields here -->
                <button class="oe_stat_button" name="moving_view" type="object" icon="fa-list-ol">
                    <field string="Moving" name="moving_count" widget="statinfo"/>
                </button>
            </xpath>
            <xpath expr="//field[@name='payment_term_id']" position="after">
              <!-- Add your fields here -->
                    <field name="typeofcontainers"/>
                    <field name="os_containers_rel" widget="many2many_tags"/>
                    <field name="os_vehicle_rel" widget="many2many_tags"/>
                    <field name="numberofcontainers"/>
                    <field name="the_total_weight"/>
                    <field name="number_of_packages"/>
                    
            </xpath>
            <xpath expr="//field[@name='partner_id']" position="after">
              <!-- Add your fields here -->
                    <field name="shippers_name"/>
                    <field name="recipients_name"/>
                    <field name="typeofshipment"/>
                    <field name="moveport"/>
                    <field name="accessport"/>
            </xpath>

      </field>
    </record>
    <!-- shipping.document form view -->
   <record id="shipping_document_view_form" model="ir.ui.view">
      <field name="name">shipping.document.view.form</field>
      <field name="model">shipping.document</field>
      <field name="arch" type="xml">
        <form string="Shipping Documents">
          <sheet>
            <group>
              <field name="name"/>
              <field name="doc_number"/>
              <field name="expiry_date"/>
            </group>
            <group>
            <field name="issue_date"/>
            </group>
            <group>
            <field name="doc_attachment_id" widget="many2many_binary" class="oe_inline"/>
            </group>
            <notebook>
                <page string="Description">
                  <field name="description"/>
                </page>
            </notebook>
          </sheet>
        </form>
      </field>
   </record> 


    <!-- shipping.document tree view -->
    <record id="shipping_document_view_tree" model="ir.ui.view">
      <field name="name">shipping.document.view.tree</field>
      <field name="model">shipping.document</field>
      <field name="arch" type="xml">
        <tree>
            <field name="name"/>
            <field name="doc_number"/>
            <field name="expiry_date"/>
        </tree>
      </field>
    </record> 

    <!-- moving.moving form view -->
    <record id="moving_moving_view_form" model="ir.ui.view">
      <field name="name">moving.moving.view.form</field>
