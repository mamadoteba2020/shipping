              <field name="moving_number"/>
              <field name="os_containers_rel_moving" widget="many2many_tags"/>
              <field name="os_vehicle_rel_moving" widget="many2many_tags"/>
              <field name="starting_point"/>
              <field name="end_point"/>
              <field name="note_moving"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
